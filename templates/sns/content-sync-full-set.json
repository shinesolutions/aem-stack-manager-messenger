{
  "externalId": "{{ externalId }}",
  "task": "content-sync",
  "source_stack_prefix": "{{ source_aem_stack_prefix }}",
  "target_stack_prefix": "{{ target_aem_stack_prefix }}",
  "details": {
    "component": [
      "author-primary",
      "author-standby",
      "publish",
      "preview-publish"
    ],
    "comment": "synchronize content between two full-set environments",
    "parameters": {
      "executionTimeout": ["14400"],
      "content_sync_enable_recursive": ["{{ content_sync_enable_recursive }}"],
      "content_sync_bathcsize": ["{{ content_sync_bathcsize }}"],
      "content_sync_enable_update": ["{{ content_sync_enable_update }}"],
      "content_sync_enable_newer_only": ["{{ content_sync_enable_newer_only }}"],
      "content_sync_exclude_path": ["{{ content_sync_exclude_path }}"],
      "content_sync_path": ["{{ content_sync_path }}"]
    }
  }
}
